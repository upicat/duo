# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=duo_db
DB_USERNAME=root
DB_PASSWORD=password

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRATION=86400

# 应用配置
APP_NAME=Duo
APP_VERSION=1.0.0
APP_ENV=development

# 前端配置
VITE_API_BASE_URL=http://localhost:8080/api
VITE_APP_TITLE=Duo-Frontend

# 后端配置
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=dev

# 邮件配置
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password

# 文件上传配置
UPLOAD_PATH=/uploads
MAX_FILE_SIZE=10MB

# 日志配置
LOG_LEVEL=INFO
LOG_PATH=./logs

# Docker配置
MYSQL_ROOT_PASSWORD=password
MYSQL_DATABASE=duo_db
MYSQL_USER=duo_user
MYSQL_PASSWORD=duo_password

# Nginx配置
NGINX_PORT=80
NGINX_SSL_PORT=443
